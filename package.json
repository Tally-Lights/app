    {
      "name": "capsia37"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@homebridge/ciao": "^1.1.2",
    "atem-connection": "^2.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "eventemitter3": "^4.0.7",
    "op-i18n": "^1.0.1101",
    "serialport": "^9.2.1",
    "update-electron-app": "^2.0.1",
    "usb-detection": "^4.10.0"
  },
  "devDependencies": {
    "electron": "14.0.0",
    "electron-builder": "^22.11.7",
