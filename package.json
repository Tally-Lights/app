{
  "name": "tally-lights",
  "productName": "tally-lights",
  "version": "v1.0.6",
  "description": "Cheap tally solution",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "tallylights",
    "productName": "Tally lights",
